{% extends "generic.djhtm" %}
{% comment %} coding: utf-8 {% endcomment %}
{% block content %}
<form enctype="multipart/form-data" action="{% url gsb.outils.import_file %}" method="post">
    <table>
        {{ form.as_table }}
    </table>
    {% csrf_token %}
